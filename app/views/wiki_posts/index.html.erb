<h1>Wiki Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th>Author</th>
    </tr>
  </thead>
  <tbody>
    <% @wiki_posts.each do |post| %>
      <tr>
        <td><%= post['title'] %></td>
        <td><%= post['created_at'] %></td>
        <td><%= post['description'] %></td>
        <td><%= post['author'].titleize %></td>
      </tr>
    <% end %>
  </tbody>
</table>